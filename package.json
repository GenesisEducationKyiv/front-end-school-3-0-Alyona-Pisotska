{
  "name": "music_app",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": "^v20.13.0"
  },
  "type": "module",
  "scripts": {
    "start": "vite",
    "typecheck": "tsc --noEmit",
    "build": "vite build && rimraf ./dist/**/*.map",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write --no-error-on-unmatched-pattern 'src/**/*.{js,jsx,ts,tsx,css,md}' *.json *.md",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:playwright": "npm run test:playwright:e2e && npm run test:playwright:component",
    "test:playwright:e2e": "playwright test",
    "test:playwright:component": "playwright test --config playwright-ct.config.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@mobily/ts-belt": "^3.13.1",
    "@playwright/experimental-ct-react": "^1.53.1",
    "@radix-ui/react-checkbox": "^1.2.2",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@sentry/react": "^9.33.0",
    "@sentry/vite-plugin": "^3.5.0",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.501.0",
    "neverthrow": "^8.2.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.0",
    "react-router-dom": "^7.5.1",
    "react-select": "^5.10.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.3",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.0.2",
    "vite": "^6.3.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  }
}
